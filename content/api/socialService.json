{
  "openapi": "3.1.1",
  "info": {
    "title": "Serviços/Programas Socioassistenciais API",
    "version": "1.0.0",
    "description": "Endpoints do módulo Serviços/Programas Socioassistenciais."
  },
  "servers": [
    {
      "url": "https://dev-api.versasocial.com.br/api/v1",
      "description": "Desenvolvimento"
    },
    {
      "url": "https://homo-api.versasocial.com.br/api/v1",
      "description": "Homologação"
    },
    {
      "url": "https://api.versasocial.com.br/api/v1",
      "description": "Produção"
    }
  ],
  "security": [{ "bearerAuth": [] }],
  "tags": [
    {
      "name": "Serviços/Programas Socioassistenciais",
      "description": "Operações do módulo Serviços/Programas Socioassistenciais"
    }
  ],
  "paths": {
    "/services": {
      "get": {
        "tags": ["Serviços/Programas Socioassistenciais"],
        "summary": "Listar serviços/programas",
        "description": "Lista paginada de serviços/programas do módulo Serviços/Programas Socioassistenciais.",
        "operationId": "listSocialServices",
        "parameters": [
          { "$ref": "#/components/parameters/X-Domain" },
          { "$ref": "#/components/parameters/page" },
          { "$ref": "#/components/parameters/per_page" },
          { "$ref": "#/components/parameters/sort_field" },
          { "$ref": "#/components/parameters/sort_order" },
          { "$ref": "#/components/parameters/search" },
          {
            "name": "name",
            "in": "query",
            "schema": { "type": "string" },
            "description": "Filtro por nome (busca parcial)"
          },
          {
            "name": "type",
            "in": "query",
            "schema": { "type": "string" },
            "description": "Filtro por tipo"
          },
          {
            "name": "acronym",
            "in": "query",
            "schema": { "type": "string" },
            "description": "Filtro por sigla (busca parcial)"
          },
          {
            "name": "offer_type",
            "in": "query",
            "schema": { "type": "string" },
            "description": "Filtro por tipo de oferta"
          },
          {
            "name": "unit_types",
            "in": "query",
            "schema": {
              "type": "array",
              "items": { "type": "string" }
            },
            "description": "Filtro por tipos de unidade"
          },
          {
            "name": "begin",
            "in": "query",
            "schema": { "type": "string", "format": "date" },
            "description": "Data inicial (maior ou igual)"
          },
          {
            "name": "end",
            "in": "query",
            "schema": { "type": "string", "format": "date" },
            "description": "Data final (menor ou igual)"
          },
          {
            "name": "created_at",
            "in": "query",
            "schema": { "type": "string", "format": "date" },
            "description": "Filtro por data de criação (igual)"
          }
        ],
        "responses": {
          "200": {
            "description": "Lista paginada de serviços/programas",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PaginatedItemResource"
                }
              }
            }
          },
          "401": { "$ref": "#/components/responses/Unauthorized" },
          "500": { "$ref": "#/components/responses/ServerError" }
        }
      },
      "post": {
        "tags": ["Serviços/Programas Socioassistenciais"],
        "summary": "Criar serviço",
        "description": "Cria um novo serviço.",
        "operationId": "createSocialService",
        "parameters": [{ "$ref": "#/components/parameters/X-Domain" }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateSocialServiceDTO"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Criado",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ItemResponse" }
              }
            }
          },
          "422": { "$ref": "#/components/responses/UnprocessableEntity" },
          "401": { "$ref": "#/components/responses/Unauthorized" }
        }
      }
    },
    "/services/plans": {
      "get": {
        "tags": ["Serviços/Programas Socioassistenciais"],
        "summary": "Listar planos",
        "description": "Lista de planos disponíveis para associação ao serviço/programa socioassistencial.",
        "operationId": "listPlans",
        "parameters": [
          { "$ref": "#/components/parameters/X-Domain" },
          {
            "name": "social_record_id",
            "in": "query",
            "schema": { "type": "string" },
            "description": "Filtro por ID do atendimento"
          },
          {
            "name": "family_id",
            "in": "query",
            "schema": { "type": "string" },
            "description": "Filtro por ID da família"
          },
          {
            "name": "ended_at",
            "in": "query",
            "schema": { "type": "boolean" },
            "description": "Filtro por planos ativos (false) ou encerrados (true)"
          },
          {
            "name": "begin",
            "in": "query",
            "schema": { "type": "string", "format": "date" },
            "description": "Data inicial (maior ou igual)"
          },
          {
            "name": "end",
            "in": "query",
            "schema": { "type": "string", "format": "date" },
            "description": "Data final (menor ou igual)"
          }
        ],
        "responses": {
          "200": {
            "description": "Lista de planos",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { "$ref": "#/components/schemas/ItemResource" }
                }
              }
            }
          },
          "401": { "$ref": "#/components/responses/Unauthorized" },
          "500": { "$ref": "#/components/responses/ServerError" }
        }
      },
      "post": {
        "tags": ["Serviços/Programas Socioassistenciais"],
        "summary": "Criar plano",
        "description": "Cria um novo plano.",
        "operationId": "createPlan",
        "parameters": [{ "$ref": "#/components/parameters/X-Domain" }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/CreateSocialServicePlanDTO" }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Criado",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ItemResponse" }
              }
            }
          },
          "422": { "$ref": "#/components/responses/UnprocessableEntity" },
          "401": { "$ref": "#/components/responses/Unauthorized" }
        }
      }
    },
    "/services/plans/tasks": {
      "post": {
        "tags": ["Serviços/Programas Socioassistenciais"],
        "summary": "Criar tarefa",
        "description": "Cria uma nova tarefa.",
        "operationId": "createPlanTask",
        "parameters": [{ "$ref": "#/components/parameters/X-Domain" }],
